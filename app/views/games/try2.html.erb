<div class="jumbotron center">
	<h1>Nope, try again</h1>
	<div class="panel-heading">
	</div>
</div>

<div class="container">
	<% chosen_song.lines[mid_num-3..mid_num+6].each do |i| %>
	<br><br> <strong id="try2" ><%=i.lyric %></strong> 
	<% end %> 
</div>
</br>


<div>
	<% list_of_answers.each do |i| %>
		<% if i == chosen_song.artist.name %>
			<div style="float:left;">
				<%= button_to i, success_game_path, :method => :get, class: "rap_btn btn btn-default btn-lg" %>
			</div>
		<% else %>
			<div style="float:left;">
				<%= button_to i, final_game_path, :method => :get, class: "rap_btn btn btn-default btn-lg" %>
			</div>
		<% end %>
	<% end %>

	<div style="clear:both;">
	</div>

</div>


<p>
	Testing...
	current artist= <%= chosen_song.artist.name %> 
	current song= <%= chosen_song.title%> 
<p>

