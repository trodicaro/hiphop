<div class="jumbotron">
	<h1>Still not it. Last try...</h1>
	<div class="panel-heading">
	</div>
</div>

<div class="container">
	<% chosen_song.lines.each do |i| %>
	 <br><strong id="final"><%=i.lyric %></strong> 
	<% end %> 
</div>
</br>


<div>
	<% list_of_answers.each do |i| %>
		<% if i == chosen_song.artist.name %>
			<div style="float:left;">
				<%= button_to i, success_game_path, :method => :get, class: "rap_btn btn btn-default btn-lg" %>
			</div>
		<% else %>
			<div style="float:left;">
				<%= button_to i, fail_game_path, :method => :get, class: "rap_btn btn btn-default btn-lg"  %>
			</div> 
		<% end %>
	<% end %>

	<div style="clear:both;">
	</div>

</div>

<p>
	Testing...
	<br>
	current artist= <%= chosen_song.artist.name %> 
	<br>
	current song= <%= chosen_song.title%> 
<p>
